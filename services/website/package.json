{
  "name": "website",
  "version": "0.0.2",
  "private": true,
  "scripts": {
    "build": "svelte-kit build",
    "clean": "rm -rf .svelte-kit ./node_modules",
    "dev": "svelte-kit dev",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
    "lint": "run-p lint:*",
    "lint:eslint": "eslint --ignore-path .gitignore .",
    "lint:svelte": "svelte-check --tsconfig ./tsconfig.json",
    "package": "svelte-kit package",
    "postbuild": "cp prisma/schema.prisma .vercel_build_output/functions/node/render/ && cp node_modules/@prisma/engines/*query* .vercel_build_output/functions/node/render/",
    "predev": "prisma migrate dev",
    "preview": "svelte-kit preview",
    "vercel-build": "prisma migrate deploy && pnpm build"
  },
  "devDependencies": {
    "@octokit/auth-oauth-user": "^1.3.0",
    "@octokit/core": "^3.6.0",
    "@sveltejs/adapter-auto": "1.0.0-next.40",
    "@sveltejs/kit": "1.0.0-next.328",
    "@tailwindcss/forms": "^0.4.0",
    "@tailwindcss/typography": "^0.5.2",
    "@types/cookie": "^0.4.1",
    "@types/cookie-parser": "^1.4.2",
    "@types/cookie-signature": "^1.0.4",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/marked": "^4.0.3",
    "@typescript-eslint/eslint-plugin": "^4.31.1",
    "@typescript-eslint/parser": "^4.31.1",
    "autoprefixer": "^10.4.2",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-svelte3": "^3.4.0",
    "postcss": "^8.4.6",
    "prettier": "^2.4.1",
    "prettier-plugin-svelte": "^2.6.0",
    "prettier-plugin-tailwindcss": "^0.1.5",
    "prisma": "^3.14.0",
    "svelte": "^3.48.0",
    "svelte-check": "^2.4.2",
    "svelte-preprocess": "^4.10.2",
    "tailwindcss": "^3.0.18",
    "tslib": "^2.3.1",
    "typescript": "^4.5.5"
  },
  "type": "module",
  "dependencies": {
    "@prisma/client": "^3.11.1",
    "cookie": "^0.4.2",
    "cookie-parser": "^1.4.6",
    "cookie-signature": "^1.1.0",
    "jsonwebtoken": "^8.5.1",
    "marked": "^4.0.12",
    "octokit": "^1.7.1",
    "php-serialize": "^4.0.2",
    "yaml": "^2.0.0-10"
  }
}
