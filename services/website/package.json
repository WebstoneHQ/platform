{
  "name": "website",
  "version": "0.0.2",
  "private": true,
  "scripts": {
    "build": "svelte-kit build",
    "clean": "rm -rf .svelte-kit ./node_modules",
    "dev": "svelte-kit dev",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
    "lint": "run-p lint:*",
    "lint:eslint": "eslint --ignore-path .gitignore .",
    "lint:svelte": "svelte-check --tsconfig ./tsconfig.json",
    "package": "svelte-kit package",
    "postbuild": "cp prisma/schema.prisma .vercel_build_output/functions/node/render/ && cp ../../node_modules/.pnpm/@prisma+client@3.14.0_prisma@3.14.0/node_modules/prisma/*engine* .vercel_build_output/functions/node/render/",
    "prebuild": "prisma generate",
    "predev": "prisma migrate dev",
    "preview": "svelte-kit preview",
    "vercel-build": "prisma migrate deploy && pnpm build"
  },
  "devDependencies": {
    "@octokit/auth-oauth-user": "^1.3.0",
    "@octokit/core": "^3.6.0",
    "@sveltejs/adapter-auto": "1.0.0-next.40",
    "@sveltejs/kit": "1.0.0-next.328",
    "@tailwindcss/forms": "^0.5.1",
    "@tailwindcss/typography": "^0.5.2",
    "@types/cookie": "^0.5.1",
    "@types/cookie-parser": "^1.4.3",
    "@types/cookie-signature": "^1.0.4",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/marked": "^4.0.3",
    "@typescript-eslint/eslint-plugin": "^5.23.0",
    "@typescript-eslint/parser": "^5.23.0",
    "autoprefixer": "^10.4.7",
    "eslint": "^8.15.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "postcss": "^8.4.13",
    "prettier": "^2.6.2",
    "prettier-plugin-svelte": "^2.7.0",
    "prettier-plugin-tailwindcss": "^0.1.10",
    "prisma": "^3.14.0",
    "svelte": "^3.48.0",
    "svelte-check": "^2.7.0",
    "svelte-preprocess": "^4.10.6",
    "tailwindcss": "^3.0.24",
    "tslib": "^2.4.0",
    "typescript": "^4.6.4"
  },
  "type": "module",
  "dependencies": {
    "@devtomio/sodium": "^0.3.0",
    "@prisma/client": "^3.14.0",
    "cookie": "^0.5.0",
    "cookie-parser": "^1.4.6",
    "cookie-signature": "^1.2.0",
    "jsonwebtoken": "^8.5.1",
    "marked": "^4.0.15",
    "octokit": "^1.7.1",
    "php-serialize": "^4.0.2",
    "yaml": "^2.0.1"
  }
}
